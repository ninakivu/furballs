<h1><%= @kitty.name %></h1>
<h2>Age: <%= @kitty.age %></h2>
<h3>Breed: <%= @kitty.breed %></h3>
<h4>Neutered: <%= @kitty.neutered %></h4>

<%= image_tag(@kitty.image) %>

<div>
  <%= link_to("Go Back", kitties_path) %>
  <%= link_to("Edit", edit_kitty_path(@kitty)) %>
  <%= link_to("Delete", kitty_path(@kitty), method: "delete", data: {confirm: "Are you sure this kitty has a new home?"}) %>
</div>

